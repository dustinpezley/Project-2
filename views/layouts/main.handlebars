<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />
    <link
      rel='stylesheet'
      href='https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.2/css/all.min.css'
      integrity='sha512-1sCRPdkRXhBV2PBLUdRb4tMg1w2YPf37qatUFeS7zlBy7jJI8Lf4VHwWfZZfpXtYSLy85pkm9GaYVYMfw5BC1A=='
      crossorigin='anonymous'
      referrerpolicy='no-referrer'
    />
    <link
      rel='stylesheet'
      href='https://cdn.jsdelivr.net/npm/foundation-sites@6.7.5/dist/css/foundation.min.css'
      crossorigin='anonymous'
    />
    {{! <link
      rel='stylesheet'
      href='https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css'
      integrity='sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx'
      crossorigin='anonymous'
    /> }}
    <link rel='stylesheet' href='/stylesheets/polyrhythm.css' />
    <title>PLACEHOLDER FOR VENUE NAME</title>
  </head>
  <body>
    {{! menu items held off-canvas }}
    <div class='off-canvas-wrapper'>
      <nav
        class='off-canvas position-left is-transition-overlap is-closed'
        id='off-canvas-menu'
        data-off-canvas
        data-transition='overlap'
        data-position='left'
        aria-hidden='true'
      >
        <a href='/' class='menu-link text-center' id='polyrhythm-link'><img
            src='/assets/images/polyrhythm.png'
            alt='polyrhythm logo with a direct link'
          /></a>
        <div>
          <a
            href='/dolce'
            target='_blank'
            referrerpolicy='noreferrer'
            class='menu-link'
            id='dolce-link'
          ><img
              src='/assets/images/dolce.png'
              alt='dolce logo with a direct link'
            /></a>
          <a
            href='/modular'
            target='_blank'
            referrerpolicy='noreferrer'
            class='menu-link'
            id='modular-link'
          ><img
              src='/assets/images/modular.png'
              alt='modular logo with a direct link'
            /></a>
          <a
            href='/harmony'
            target='_blank'
            referrerpolicy='noreferrer'
            class='menu-link'
            id='harmony-link'
          ><img
              src='/assets/images/harmony.png'
              alt='harmony logo with a direct link'
            /></a>
          <a
            href='/calendar'
            target='_blank'
            referrerpolicy='noreferrer'
            class='menu-link'
            id='calendar-link'
          ><i class='fa-duotone fa-calendar-days'></i></a>
        </div>
        <a
          href='/admin'
          target='_blank'
          referrerpolicy='noreferrer'
          class='menu-link'
          id='admin-link'
        ><i class='fa-duotone fa-lock'></i></a>
      </nav>
      {{! everything not in the menu, including the menu button }}
      <main>
        <div class='off-canvas-content' data-off-canvas-content>
          <button
            type='button'
            class='button menu-btn'
            data-toggle='off-canvas-menu'
            aria-expanded='false'
            aria-controls='off-canvas-menu'
          ><lottie-player
              src='https://assets2.lottiefiles.com/private_files/lf30_fah4ouxp.json'
              background='transparent'
              speed='1'
              style='width: 100px; height: 100px;'
              hover
              loop
            ></lottie-player></button>
          {{{body}}}
        </div>
      </main>
    </div>
    {{! <script
      src='https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.min.js'
      integrity='sha384-ODmDIVzN+pFdexxHEHFBQH3/9/vQ9uori45z4JjnFsRydbmQbmL5t1tQ0culUzyK'
      crossorigin='anonymous'
    ></script> }}
    <script
      src='https://cdn.jsdelivr.net/npm/foundation-sites@6.7.5/dist/js/foundation.min.js'
      crossorigin='anonymous'
    ></script>
    <script src='/javascript/script.js' type='module'></script>
  </body>
</html>